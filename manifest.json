{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["openpgp.js", "keys.js", "content.js"]
    }
  ],
  "background": {
    "scripts": [ "back.js" ],
    "persistent": false
  },

  // "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources" : [
    "back.html"
  ],
  "browser_action": {
    "default_title": "Test Extension"
  },

  "permissions": ["activeTab"]
}